<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tarabut.nbbpayments">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/budget"
        android:label="@string/app_name"
        android:roundIcon="@drawable/budget"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.tarabut.nbbpayments.TGPayment"></activity>
        <activity android:name="com.tarabut.nbbpayments.CreateTransferTokenSample" />
        <activity android:name="com.tarabut.nbbpayments.Pay_fill" />
        <activity
            android:name="com.tarabut.nbbpayments.NewDashboard"
            android:label="Dashboard" />
        <activity
            android:name="com.tarabut.nbbpayments.LauncherActivity"
            android:theme="@style/MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tarabut.nbbpayments.Dashboard"
            android:label="Login" />
    </application>

</manifest>